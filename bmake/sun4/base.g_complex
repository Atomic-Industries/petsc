CLINKER    = g++ $(COPTFLAGS)
FLINKER    = g++ $(COPTFLAGS)
CC         = g++  -DPETSC_COMPLEX -DPARCH_sun4  -fPIC
PETSCFLAGS = -DPETSC_DEBUG  -DPETSC_LOG -DPETSC_BOPT_g -DPETSC_COMPLEX  \
             -DUSES_TEMPLATED_COMPLEX
COPTFLAGS  = -g 
FOPTFLAGS  = -g -dalign -DPARCH_sun4
SYS_LIB  = /usr/lib/debug/malloc.o /usr/lib/debug/mallocmap.o
