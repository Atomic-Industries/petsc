CC	       = ${C_CC}
FC	       = ${C_FC}
CLINKER_SLFLAG = ${C_CLINKER_SLFLAG}
FLINKER_SLFLAG = ${C_FLINKER_SLFLAG}
CLINKER	       = ${C_CLINKER} ${COPTFLAGS} ${CLINKER_SLFLAG}${LDIR}:${DYLIBPATH}
FLINKER	       = ${C_FLINKER} ${FOPTFLAGS} ${FLINKER_SLFLAG}${LDIR}:${DYLIBPATH}
SYS_LIB	       = ${C_SYS_LIB}

PETSCFLAGS     = ${O_PETSCFLAGS}  -DUSE_MAT_SINGLE
COPTFLAGS      = ${O_COPTFLAGS}
FOPTFLAGS      = ${O_FOPTFLAGS}
