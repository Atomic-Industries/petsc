#! /bin/csh
# $Id: makefile,v 1.23 1997/08/22 15:15:29 bsmith Exp $ 

# 
#  Removes PETSc libraries on some nightly build machines
#
rsh elvis -n /bin/rm -f "/tmp/petsc/lib/lib*/freebsd/*"
rsh violet -n /bin/rm -f "/tmp/petsc/lib/lib*/IRIX/*"
rsh maverick -n /bin/rm -f "/tmp/petsc/lib/lib*/sun4*/*.so.* /tmp/petsc/lib/lib*/sun4_uni/*"
/bin/rm -f /home/curfman/petsc/lib/lib*/alpha/* /home/petsc/petsc/lib/lib*/hpux/* /home/balay/petsc/lib/lib*/solaris/*


