#!/bin/csh
# $Id: removelibs,v 1.32 1998/07/11 20:46:58 balay Exp balay $ 

# 
#  Removes PETSc libraries on some nightly build machines
#
rsh snowy    -n /bin/rm -f "/tmp/petsc/petsc/lib/lib*/freebsd*/*"
rsh violet   -n /bin/rm -f "/tmp/petsc/petsc/lib/lib*/IRIX/*"
rsh alaska   -n /bin/rm -f "/tmp/petsc/petsc/lib/lib*/IRIX/*"
rsh ptera    -n /bin/rm -f "/tmp/petsc/petsc/lib/lib*/alpha*/*"
rsh maverick -n /bin/rm -f "/tmp/petsc/petsc/lib/lib*/sun4*/*"
rsh fire     -n /bin/rm -f "/sandbox/balay/petsc/lib/lib*/solaris*/*"
rsh gaea     -n /bin/rm -f "/sandbox/petsc/petsc/lib/lib*/linux/*.so*"
rsh yukon    -n /bin/rm -f "/sandbox/petsc/petsc/lib/lib*/IRIX64/*.so*"
/bin/rm -f /home/petsc/petsc/lib/lib*/hpux/*


