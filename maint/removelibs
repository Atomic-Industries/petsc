#! /bin/csh
# 
#  Removes PETSc libraries on some nightly build machines
#
rsh elvis -n /bin/rm -f "/tmp/petsc/lib/lib*/freebsd/*"
/bin/rm -f "/home/curfman/petsc/lib/lib*/alpha/*"
rsh cyan -n /bin/rm -f "/tmp/petsc/lib/lib*/IRIX/*"
rsh maverick -n /bin/rm -f "/tmp/petsc/lib/lib*/sun4*/*.so.*"
rsh maverick -n /bin/rm -f "/tmp/petsc/lib/lib*/sun4_uni/*"
/bin/rm -f "/home/petsc/petsc/lib/lib*/hpux/*"
/bin/rm -f "/home/balay/petsc/lib/lib*/solaris/*"


