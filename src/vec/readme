int VecASum          (Vec, Scalar*);
int VecMax           (Vec, int *,Scalar*);
int VecScale         (Scalar*, Vec);    
int VecCopy          (Vec, Vec);        
int VecSet           (Scalar*, Vec);    
int VecSwap          (Vec, Vec);
int VecAXPY          (Scalar*, Vec, Vec);  
int VecMAXPY         (int, Scalar*, Vec ,Vec*);
int VecAYPX          (Scalar*, Vec, Vec);
int VecWAXPY         (Scalar*, Vec, Vec, Vec);
int VecPMult         (Vec, Vec, Vec);    
int VecPDiv          (Vec, Vec, Vec);    
int VecCreate        (MPI_Comm,int,Vec);          
int VecDestroy       (Vec);        
int VecGetVecs       (Vec, int,Vec**);         
int VecFreeVecs      (Vec*,int); 

