reset

#{nx = 50}

brick x 1
volume 1 move x 0.5 y 0.5 z 0.5
block 1 volume 1

surface 2 name "BackSurface"
surface 3 name "BottomSurface"
surface 1 name "FrontSurface"
surface 4 name "LeftSurface"
surface 5 name "RightSurface"
surface 6 name "TopSurface"

surface all interval {nx}
set source surface pattern "FrontSurface"
set target surface pattern "BackSurface"
volume 1 scheme sweep  
mesh volume all

export mesh "./3Dbrick.exo" dimension 3 block 1 overwrite

exit

