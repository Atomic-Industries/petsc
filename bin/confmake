#! /bin/sh
PATH=/home/gropp/bin:$PATH
export PATH
autoconf -l config
/bin/rm -f .tmp
sed -e 's/PAC_CHECK_HEADERS/AC_HAVE_HEADERS/g' \
    -e 's/PAC_CHECK_HEADER/AC_HAVE_HEADERS/g' configure.in > .tmp
autoheader -l config .tmp | sed -e 's/\.tmp/configure.in/g' > petscconf.h.in
/bin/rm -f .tmp
#maint/splitsed < configure > configure.2
#chmod a+x configure.2
