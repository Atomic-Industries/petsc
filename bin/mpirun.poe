#!/usr/local/bin/bash
# $Id: makefile,v 1.23 1997/08/22 15:15:29 bsmith Exp $ 

#
# This script is for IBM SP machines running POE that do not 
# have mpirun installed. 
#
# Usage: petsc/bin/mpirun.poe [-np n] programname programoptions
#
two=$2
if [ $1 = "-np" ]; then
        shift
        shift
        /usr/bin/poe $* -procs $two
else
        /usr/bin/poe $*
fi

